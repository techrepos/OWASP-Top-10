{
  "https_port": 443,
  "Serilog": {
    "MinimumLevel": "Information",
    "Override": {
      "Microsoft.AspNetCore": "Error"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Serilogs\\AppLogs.log"
        }
      }
    ]
  },
  "AuthMessageSenderOptions": {
    "SendGridUser": "",
    "SendGridKey": ""

  },
  "AllowedHosts": "*"
 
}